{
  "name": "okta-superblocks-embed",
  "version": "1.0.0",
  "description": "A repository for integrating Superblocks with custom authentication using Okta.",
  "scripts": {
    "install": "npm install --prefix ./app && npm install --prefix ./lambda",
    "start": "concurrently \"npm run dev:lambda\" \"npm run dev:app\"",
    "dev:app": "npm run dev --prefix ./app",
    "dev:lambda": "npm run sam:local --prefix ./lambda",
    "build:app": "npm run build --prefix ./app",
    "build:lambda": "npm run sam:build --prefix ./lambda",
    "build": "concurrently \"npm run build:app\" \"npm run build:lambda\"",
    "invoke:lambda": "npm run sam:invoke --prefix ./lambda"
  },
  "author": "Kris Harbold",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
